
@{
    ViewData["Title"] = "AddBasicInformation";
}
<div class="row">
    <div class="col-lg-12 col-md-12 col-sm-12">
        @using (Html.BeginForm())
    {
        <div class="form-group">
            @Html.Label("Informacja")
            @Html.TextBox("info.Information")
        </div>
        <div class="form-group">
            @Html.Label("Dane")
            @Html.TextBox("info.Data")
        </div>
        <input type="submit" value="Dodaj" />
    }
    </div>
</div>

